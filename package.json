{
  "name": "node-express-starter-mongodb",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "scripts": {
    "start": "cross-env NODE_ENV=production pm2 start dist/server.js",
    "stop": "pm2 start dist/server.js",
    "build": "tsc --project ./",
    "dev": "npm-run-all --parallel dev:*",
    "dev:server": "cross-env NODE_ENV=development tsnd --respawn server.ts",
    "setup": "npm install && npm run validate",
    "check-types": "tsc",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts .",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|ts|json)\"",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "validate": "npm-run-all --parallel check-types check-format lint build",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "upgrade-interactive": "npx npm-check --update"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-mongo-sanitize": "^2.0.0",
    "express-pino-logger": "^5.0.0",
    "express-rate-limit": "^5.1.3",
    "express-validator": "^6.6.1",
    "helmet": "^4.1.1",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^5.10.5",
    "morgan": "^1.10.0",
    "node-geocoder": "^3.27.0",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "pino-noir": "^2.2.1",
    "pm2": "^4.4.1",
    "supertest": "^4.0.2",
    "validator": "^13.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-proposal-numeric-separator": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.6",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.8",
    "@types/express-mongo-sanitize": "^1.3.2",
    "@types/express-pino-logger": "^4.0.2",
    "@types/express-rate-limit": "^5.1.0",
    "@types/helmet": "0.0.48",
    "@types/hpp": "^0.2.1",
    "@types/jest": "^26.0.14",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongoose": "^5.7.36",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.11.1",
    "@types/node-geocoder": "^3.24.1",
    "@types/passport": "^1.0.4",
    "@types/passport-local": "^1.0.33",
    "@types/validator": "^13.1.0",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "is-ci-cli": "^2.1.2",
    "jest": "^26.4.2",
    "lint-staged": "^10.4.0",
    "mock-req-res": "^1.2.0",
    "npm": "^6.14.8",
    "npm-check": "^5.9.2",
    "npm-run-all": "^4.1.5",
    "pino-pretty": "^4.2.1",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.3.0",
    "ts-node-dev": "^1.0.0-pre.56",
    "typescript": "^3.9.7"
  },
  "jest": {
    "verbose": true,
    "testURL": "http://localhost/",
    "testEnvironment": "node",
    "restoreMocks": true,
    "roots": [
      "<rootDir>/"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(j|t)sx?$",
    "testPathIgnorePatterns": [
      "dist/"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/utils/setupTests.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "<rootDir>/tsconfig.json"
      }
    }
  }
}
